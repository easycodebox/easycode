			<description><![CDATA[已知枚举的值，例：OpenClose.OPEN]]></description>
			<name>enumValue</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[比较枚举值的某个属性，null值代表比较枚举对象本身]]></description>
			<name>enumProperty</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<dynamic-attributes>false</dynamic-attributes>
