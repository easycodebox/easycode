	<tag>
		<description><![CDATA[Render a formatted number.]]></description>
		<name>number</name>
		<tag-class>com.easycodebox.common.tag.Number</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[The number value to format]]></description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Type of number formatter (currency, integer, number or percent, DecimalFormat pattern. default is 0.##)]]></description>
			<name>pattern</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Maximum fraction digits]]></description>
			<name>maxFractionDigits</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Maximum integer digits]]></description>
			<name>maxIntDigits</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Minimum fraction digits]]></description>
			<name>minFractionDigits</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Maximum integer digits]]></description>
			<name>minIntDigits</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Whether grouping is used, default is false]]></description>
			<name>groupUsed</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[groupingSize]]></description>
			<name>groupSize</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[Parse integer only]]></description>
			<name>onlyInt</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[默认是DOWN（直接舍去多余的小数位，不四舍五入）。UP/DOWN/CEILING/FLOOR/HALF_UP/HALF_DOWN/HALF_EVEN ]]></description>
			<name>roundingMode</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<dynamic-attributes>false</dynamic-attributes>
	</tag>
	
	<tag>
		<description><![CDATA[Convert ID to expected value.]]></description>
		<name>convertId</name>
		<tag-class>com.easycodebox.common.tag.ConvertIdTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[Need to convert value.]]></description>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[The module of ID]]></description>
			<name>module</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
