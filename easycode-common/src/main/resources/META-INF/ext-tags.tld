        version="2.0">
	
	<description><![CDATA[项目中使用到的一些标签，方便表现层的显示]]></description>
	<tlib-version>4.0</tlib-version>
	<short-name>e</short-name>
	<uri>/ext-tags</uri>

	<tag>
		<description><![CDATA[显示枚举类的信息]]></description>
		<name>showEnum</name>
		<tag-class>com.easycodebox.common.tag.ShowEnum</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description><![CDATA[枚举用很么类型的方法转换出需要的值，1.valToDesc 2.objToVal 3.objToDesc 4.descToVal]]></description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>
				<![CDATA[1.valToDesc 时value为Enum的value字段值 2.objToVal 时value为Enum对象名
						3.objToDesc 时value为Enum对象名 4.descToVal 时value为Enum的desc字段名]]>
			</description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>
				<![CDATA[枚举类类型]]>
			</description>
			<name>enumName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<description><![CDATA[Iterate over a iterable Enum value,添加include==>过滤exclude==>过滤begin-end]]></description>
		<name>iteratorEnum</name>
		<tag-class>com.easycodebox.common.tag.IteratorEnum</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[begin index of IteratorEnum]]></description>
			<name>begin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[end index of IteratorEnum]]></description>
			<name>end</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>enumName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>status</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>step</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>
			  <![CDATA[排除的枚举值，例如：ABC,ICBC]]>
			</description>
			<name>exclude</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>
			  <![CDATA[需要的枚举值，例如：ABC,ICBC]]>
			</description>
			<name>include</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<description><![CDATA[If tag]]></description>
		<name>ifEnum</name>
		<tag-class>com.easycodebox.common.tag.IfEnum</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[被比较的表达式]]></description>
			<name>test</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[已知枚举的值，例：OpenClose.OPEN]]></description>
			<name>enumValue</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[比较枚举值的某个属性，null值代表比较枚举对象本身]]></description>
			<name>enumProperty</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<dynamic-attributes>false</dynamic-attributes>
	</tag>
	
	<tag>
		<description><![CDATA[Elseif tag]]></description>
		<name>elseIfEnum</name>
		<tag-class>com.easycodebox.common.tag.ElseIfEnum</tag-class>
