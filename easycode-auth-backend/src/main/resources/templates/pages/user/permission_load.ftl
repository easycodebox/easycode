<table id="loadDialogPermission" class="table-edit none">
	
	<tr>
		<th>
			<label>上级权限：</label>
		</th>
		<td>
			{{permission.parentName}}
		</td>
		<th>
			<label>项目：</label>
		</th>
		<td>
			{{permission.projectName}}
		</td>
	</tr>
	
	<tr>
		<th>
			<label>权限名：</label>
		</th>
		<td>
			{{permission.name}}
		</td>
		<th>
			<label>状态：</label>
		</th>
		<td>
			{{permission.status.desc}}
		</td>
	</tr>
	
	<tr>
		<th>
			<label>菜单：</label>
		</th>
		<td>
			{{permission.isMenu.desc}}
		</td>
		<th>
			<label>地址：</label>
		</th>
		<td>
			{{permission.url}}
		</td>
	</tr>
	
	<tr>
		<th>
			<label>排序值：</label>
		</th>
		<td>
			{{permission.sort}}
		</td>
		<th>
			<label>图标：</label>
		</th>
		<td>
			{{permission.icon}}
		</td>
	</tr>
	
	<tr>
		<th>
			<label>创建人：</label>
		</th>
		<td>
			{{permission.creatorName}}
		</td>
		<th>
			<label>创建时间：</label>
		</th>
		<td>
			{{permission.createTime | date}}
		</td>
	</tr>
	
	<tr>
		<th>
			<label>修改人：</label>
		</th>
		<td>
			{{permission.modifierName}}
		</td>
		<th>
			<label>修改时间：</label>
		</th>
		<td>
			{{permission.modifyTime | date}}
		</td>
	</tr>
	<tr>
		<th>
			<label>描述：</label>
		</th>
		<td colspan="3">
			<textarea class="form-control" readonly="readonly" autocomplete="off">{{permission.description}}</textarea>
		</td>
	</tr>
	<tr>
		<th>
			<label>备注：</label>
		</th>
		<td colspan="3">
			<textarea class="form-control" readonly="readonly" autocomplete="off">{{permission.remark}}</textarea>
		</td>
	</tr>
</table>
